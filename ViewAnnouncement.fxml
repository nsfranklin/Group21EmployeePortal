<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AnnouncementController">
    <children>
        <Pane fx:id="pane" layoutY="-1.0" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Text layoutX="153.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Announcement Board">
                    <font>
                        <Font name="Arial Black" size="25.0" />
                    </font>
                </Text>
                <Text layoutX="40.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Title:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Text>
                <TextField fx:id="title" layoutX="93.0" layoutY="75.0" prefHeight="25.0" prefWidth="436.0" />
                <Text layoutX="40.0" layoutY="128.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Message:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Text>
                <TextArea fx:id="message" layoutX="93.0" layoutY="115.0" prefHeight="161.0" prefWidth="436.0" />
                <Text layoutX="41.0" layoutY="301.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Date:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Text>
                <Label fx:id="date" layoutX="93.0" layoutY="288.0" prefHeight="17.0" prefWidth="189.0" text="Label" />
                <Label fx:id="time" layoutX="94.0" layoutY="321.0" prefHeight="17.0" prefWidth="103.0" text="Label" />
                <Text layoutX="40.0" layoutY="332.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Text>
                <Button fx:id="submit" layoutX="462.0" layoutY="340.0" mnemonicParsing="false" onAction="#submitClicked" prefHeight="44.0" prefWidth="67.0" text="Submit">
                    <effect>
                        <SepiaTone />
                    </effect>
                </Button>
            </children>
        </Pane>
    </children>
</AnchorPane>
